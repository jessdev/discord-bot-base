{
  "name": "replace-my-name",
  "version": "1.0.0",
  "description": "The base for the discord bot that is typescript based. Settings for jest unit tests",
  "main": "dist/bot.js",
  "scripts": {
    "test": "node ./node_modules/jest/bin/jest.js",
    "lint": "node ./node_modules/tslint/bin/tslint -p ./tsconfig.json",
    "lintFix": "node ./node_modules/tslint/bin/tslint -p ./tsconfig.json --fix",
    "build": "node ./node_modules/typescript/bin/tsc -p ./tsconfig.json",
    "watch": "node ./node_modules/typescript/bin/tsc --watch",
    "prestart": "npm run build",
    "start": "node ./dist/bot.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jessdev/discord-bot-base.git"
  },
  "keywords": [
    "discord",
    "amongus"
  ],
  "author": "jessdev@umich.edu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jessdev/discord-bot-base"
  },
  "homepage": "https://github.com/jessdev/discord-bot-base#readme",
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.0.27",
    "@types/sqlite3": "^3.1.6",
    "jest": "^26.4.2",
    "ts-jest": "^26.4.0",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@types/uuid": "^8.3.0",
    "discord.js": "^12.3.1",
    "sqlite3": "^5.0.0",
    "uuid": "^8.3.0"
  }
}
